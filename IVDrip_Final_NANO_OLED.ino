void setup() {
  // put your setup code here, to run once:
  Serial.begin(115200);

  for (int thisReading = 0; thisReading < numReadings; thisReading++) {
    readings[thisReading] = 0;
  }
  
  Wire.begin();
  
  pinMode(sensor,INPUT);
  pinMode(ledPin, OUTPUT);
//  pinMode(dropLedPin, OUTPUT);
  digitalWrite(ledPin,HIGH);
  pinMode(battery,INPUT);
  pinMode(buttonPin, INPUT_PULLUP);
  pinMode(buzzer,OUTPUT);
  pinMode(resetButton,INPUT_PULLUP);

  display.begin(SSD1306_SWITCHCAPVCC, SCREEN_ADDRESS); // start transmission to OLED
  display.ssd1306_command(SSD1306_SETCONTRAST); // set brightness to minimum to conserve battery
  display.ssd1306_command(1);
  
  display.clearDisplay(); //for Clearing the display
  display.drawBitmap(0, 0, myBitmap, 128, 64, WHITE); // draw 3logytech bitmap
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(5,0);
  display.print("Trilogy Technologies");
  display.display();
  delay(5000);
}

// function to display the drip rate
void displayDripRate(){
  display.clearDisplay(); // clear display
  display.setTextColor(WHITE); // settings color
  display.setTextSize(2); // sets text size
  display.setCursor(0,0); // sets cursor coordinates
  display.print("DripRate");
  display.setTextSize(1);
  display.setCursor(0,20);
  display.print("DPS: ");
  display.print(dropsPerSecond);
  display.print(" DPM: ");
  display.print(dropsPerMinute);
  display.setCursor(0,40);
  display.print("Drop No: " );
  display.print(counter);
}

void loop(){

  sensorReading = analogRead(sensor); // read sensor 

