    }

    if (resetButtonState != rLastButtonState && resetButtonState == LOW) {
      Serial.println('Reset!');
      buzzerOn = false;
      for (int i = 0; i < dropnumber ; i++){
        droptimes[i] = 0;
      }
      counter = 0;
      dropIndexNo = 0;
      highestDT = 0;
      lowestDT = 0;
      timePerDrop = 0;
      dropCounter = 0;
      dropsPerSecond = 0;
      dropsPerMinute = 0;      
    }
    // save the current state as the last state, for next time through the loop
    lastButtonState = buttonState;
    rLastButtonState = resetButtonState;
  }

  if (displayState){
    displayDripRate(); // displays drip rate
      if (voltage <= 3.3){ // change batteryIndicator according to battery level
        batteryIndicator[0] = 1; 
        batteryIndicator[1] = 0;
        batteryIndicator[2] = 0;
        batteryIndicator[3] = 0;
      } // dead battery code

      else if (voltage <= 3.73 && voltage >= 3.4){
        batteryIndicator[0] = 0;
        batteryIndicator[1] = 1;
        batteryIndicator[2] = 0;
        batteryIndicator[3] = 0;
      } // flat battery code

      else if (voltage <= 3.85 && voltage >= 3.77){
        batteryIndicator[0] = 0;
        batteryIndicator[1] = 0;
        batteryIndicator[2] = 1;
        batteryIndicator[3] = 0;
      } // almost batt code

      else if (voltage >= 3.89){
        batteryIndicator[0] = 0;
        batteryIndicator[1] = 0;
        batteryIndicator[2] = 0;
        batteryIndicator[3] = 1;

      } // full batt code
      
      if (batteryIndicator[0]){ // display battery according to batteryIndicator
        display.drawBitmap(0, 0, deadBitmap, 128, 64, WHITE); // display.drawBitmap(x position, y position, bitmap data, bitmap width, bitmap height, color)
      } else if (batteryIndicator[1]){
        display.drawBitmap(0, 0, flatBitmap, 128, 64, WHITE); // display.drawBitmap(x position, y position, bitmap data, bitmap width, bitmap height, color)
      } else if (batteryIndicator[2]){
        display.drawBitmap(0, 0, almostBitmap, 128, 64, WHITE); // display.drawBitmap(x position, y position, bitmap data, bitmap width, bitmap height, color)
      } else if (batteryIndicator[3]){
        display.drawBitmap(0, 0, fullBitmap, 128, 64, WHITE); // display.drawBitmap(x position, y position, bitmap data, bitmap width, bitmap height, color)
      }
      display.display();
  };

  // buzzer code
  dPMDiff = dropsPerMinute - prevDPM; 
  if (counter >= 100 && (abs(dPMDiff) >= (20+dropsPerMinute/10) || millis() - dropDetTime >= 10000)){ // turn on buzzer when more than 100 drops, and either diffDPM has changed or theres no drops coming
    buzzerOn = true;
  }

  if (buzzerOn){
    buzzerMillis = millis();
    if (buzzerMillis - pBuzzerMillis >= 1000){
        pBuzzerMillis = buzzerMillis;
        buzzerPlay = !buzzerPlay;
        if (buzzerPlay){
          tone(buzzer,1000); // buzz the buzzer    
        } else {
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             noTone(buzzer);
        }
    }
    //digitalWrite(buzzer,HIGH);
  } else {
    //digitalWrite(buzzer,LOW);
    noTone(buzzer); // stop buzzing the buzzer
  }
//  Serial.println("dropsPerMinute:" + String(dropsPerMinute));
//  Serial.println("dropsPerMinute: " + String(dropsPerMinute)+ " | prevDPM: " + String(prevDPM) + " | dPMDiff: " + String(abs(dPMDiff)));
  prevDPM = dropsPerMinute;

//  Serial.println(voltage);
  delay(1);
}
